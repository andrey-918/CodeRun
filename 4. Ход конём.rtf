def get_answer(n, m):\
    dp = [[0 for i in range(m + 2)] for j in range (n + 2)]\
    dp[1][1] = 1\
    for i in range (1, n + 1):\
        for j in range (1, m + 1):\
            if i == j and i == 1:continue\
            dp[i][j] = dp[i - 1][j - 2] + dp[i - 2][j - 1]\
    return dp[n][m]\
n, m = map(int, input().split())\
print(get_answer(n, m))}
